    "version": "0.2.0",
    "configurations": [
        {
            "name": "RadI0 DAB 8C  gain vlc",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-f", "8C", "-g", "-200"],
            "console": "externalTerminal"
        },
        {
            "name": "RadI0 FM 104 MHz",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-fm", "-f", "104MHz" ],
            "console": "externalTerminal"
        },
        {
            "name": "RadI0 DAB 8C",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-dab", "-f", "8C" ],
            "console": "externalTerminal"
        },
        {
            "name": "RadI0 DAB 8C, service 4059",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-dab", "-f", "8C", "-sn" , "4059" ],
