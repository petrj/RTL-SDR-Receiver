    "version": "0.2.0",
    "configurations": [
        {
            "name": "DAB 8C",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-f", "8C"],
            "console": "externalTerminal"
        },
        {
            "name": "DAB from /temp/DAB.raw",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-dab", "-ifile", "/temp/DAB.raw"],
            "console": "externalTerminal"
        },
        {
            "name": "FM from /temp/FM.raw",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-fm", "-ifile", "/temp/FM.raw"],
            "console": "externalTerminal"
        },
        {
            "name": "FM 104 MHz",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-fm", "-f", "104MHz" ],
            "console": "externalTerminal"
        },
        {
            "name": "DAB 8C SW gain",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-dab", "-f", "8C", "-sgain" ],
            "console": "externalTerminal"
        },
        {
            "name": "DAB 8C, service 4059",
            //"type": "dotnet",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildRadI0",
            "program": "${workspaceFolder}/RadI0/bin/Debug/net10.0/RadI0.dll",
            "args": ["-dab", "-f", "8C", "-sn" , "4059" ],
            "console": "externalTerminal"
        }
    ]
}